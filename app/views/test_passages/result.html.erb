<% if @test_passage.successful? %>
  <h1 style='color: green'>The <%= @test_passage.test.title %> Test was Successfuly Completed</h1>
<% else %>
  <h1 style='color: red'>The <%= @test_passage.test.title %> Test wasn't Completed</h1>
<% end %>
<p><font size="4">
  You have decided correctly <%= @test_passage.correct_questions %> of <%= @test_passage.test.questions.count %>
</font></p>

<%= link_to 'Tests', tests_path %>
